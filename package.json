{
  "name": "definitely",
  "version": "1.0.6",
  "description": "Disallow undefined property access in JavaScript",
  "main": "definitely.js",
  "scripts": {
    "build": "babel --minified -o definitely.js src/definitely.js",
    "test": "mocha --harmony-proxies --reporter progress --compilers js:babel-core/register src/*.test.js",
    "test:watch": "npm run test -- --watch",
    "release": "xyz",
    "release:patch": "xyz --increment patch",
    "release:minor": "xyz --increment minor",
    "release:major": "xyz --increment major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pelotom/definitely.git"
  },
  "keywords": [
    "object",
    "property",
    "undefined",
    "proxy"
  ],
  "author": "Tom Crockett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pelotom/definitely/issues"
  },
  "homepage": "https://github.com/pelotom/definitely#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "harmony-proxy": "^1.0.1",
    "mocha": "^3.0.2",
    "xyz": "^1.0.0"
  }
}
